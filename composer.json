{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Protus 3",
  "keywords": [
    "Protus"
  ],
  "homepage": "http://ait.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "http://ait.com/"
  },
  "minimum-stability": "stable",
  "repositories": {
    "ait-citrix": {
      "type": "git",
      "url": "git@gitlab.ait.com:ait/citrix.git"
    },
    "ait-guzzle": {
      "type": "git",
      "url": "git@gitlab.ait.com:ait/guzzle.git"
    },
    "ait-keystorage": {
      "type": "git",
      "url": "git@gitlab.ait.com:ait/keystorage.git"
    },
    "ait-extlib": {
      "type": "package",
      "package": {
        "version": "1.0.1",
        "name": "bower/ext-lib",
        "source": {
          "type": "git",
          "url": "git@gitlab.ait.com:ait/extlib.git",
          "reference": "1.0.1"
        }
      }
    }
  },
  "require": {
    "php": ">=5.5.9",
    "yiisoft/yii2": "~2.0.10",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-jui": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.0",
    "mdmsoft/yii2-admin": "~2.0",
    "kartik-v/yii2-widget-depdrop": "^1.0.0",
    "ait/keystorage": "0.0.2",
    "bower/ext-lib": "^1.0.1",
    "yii2tech/ar-linkmany": "^1.0",
    "ait/citrix": "1.0.9"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.0.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1",
    "c006/yii2-migration-utility": "^1.2"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "scripts": {
    "post-create-cmd": "php init --env=Development --overwrite=n",
    "post-update-cmd": [
      "php yii migrate --interactive=0"
    ],
    "post-install-cmd": [
      "php yii migrate --interactive=0"
    ]
  }
}
