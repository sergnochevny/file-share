{
  "name": "ait/protus",
  "description": "Protus 3",
  "keywords": [
    "Protus"
  ],
  "homepage": "http://ait.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "http://ait.com/"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5.9",
    "yiisoft/yii2": "~2.0.10",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-jui": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.0",
    "mdmsoft/yii2-admin": "~2.0",
    "kartik-v/yii2-widget-depdrop": "^1.0.0",
    "ait/keystorage": "^1.0.2",
    "bower/ext-lib": "^1.0.1",
    "yii2tech/ar-linkmany": "^1.0",
    "ait/citrix": "^1.0.17",
    "kartik-v/yii2-password": "^1.5",
    "ait/yii2-fileupload":"^1.0.2",
    "yiisoft/yii2-coding-standards": "^2.0",
    "ait/auth": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.0.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1",
    "c006/yii2-migration-utility": "^1.2"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  },
  "scripts": {
    "post-create-cmd": "php init --env=Development --overwrite=n",
    "post-update-cmd": [
      "php yii migrate --interactive=0",
      "rm -rf backend/web/assets/*"
    ],
    "post-install-cmd": [
      "php yii migrate --interactive=0",
      "rm -rf backend/web/assets/*"
    ]
  }
}
